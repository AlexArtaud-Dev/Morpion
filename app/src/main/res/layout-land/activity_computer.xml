<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#230140"
    tools:context=".ComputerActivity">

    <TextView
        android:id="@+id/playingPlayerComp"
        android:layout_width="297dp"
        android:layout_height="129dp"
        android:fontFamily="@font/nasalizationrg"
        android:text="@string/turn"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout8"
        android:layout_width="333dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@+id/divider6"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/button_01"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="12dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/button_02"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="1dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toEndOf="@+id/button_01"
            app:layout_constraintTop_toTopOf="@+id/button_01" />

        <ImageButton
            android:id="@+id/button_03"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="20dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toEndOf="@+id/button_02"
            app:layout_constraintTop_toTopOf="@+id/button_02" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout9"
        android:layout_width="333dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@+id/divider6"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout8">

        <ImageButton
            android:id="@+id/button_04"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="19dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/button_05"
            app:layout_constraintTop_toTopOf="@+id/button_05" />

        <ImageButton
            android:id="@+id/button_05"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="12dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/button_06"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="23dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            android:textSize="60sp"
            app:layout_constraintStart_toEndOf="@+id/button_05"
            app:layout_constraintTop_toTopOf="@+id/button_05" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="332dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@+id/divider6"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout9">

        <ImageButton
            android:id="@+id/button_07"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="19dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/button_08"
            app:layout_constraintTop_toTopOf="@+id/button_08" />

        <ImageButton
            android:id="@+id/button_08"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="12dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/button_09"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="23dp"
            android:background="@color/primary_color"
            android:onClick="playerPlay"
            android:scaleType="fitXY"
            app:layout_constraintStart_toEndOf="@+id/button_08"
            app:layout_constraintTop_toTopOf="@+id/button_08" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/divider6"
        android:layout_width="10dp"
        android:layout_height="420dp"
        android:layout_marginStart="27dp"
        android:background="@color/primary_color"
        app:layout_constraintStart_toEndOf="@+id/playingPlayerComp"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
